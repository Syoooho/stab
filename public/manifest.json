{
  "manifest_version": 3,
  "name": "Stab - Smart Tab",
  "version": "1.0.0",
  "description": "Minimalist Intelligent New Tab Page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "geolocation",
    "alarms",
    "favicon"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  }
}
